<% if File.exist?(Rails.root.join("app/dfdata/region1-00550-01-01-legends.xml")) %>
  <div class="center jumbotron">
    <h1>Artifacts</h1>
  </div>
  <div class="center jumbotron">
    <h2>
      <%= render partial: "artifact", collection: @artifacts %>
    </h2>
  </div>
<% else %>
  <div class="center jumbotron">
    <h1>No Export Files Loaded</h1>
    <%= link_to "Upload Files", '#', class: "btn btn-lg btn-primary" %>
  </div>
<% end %>
